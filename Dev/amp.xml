<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Bjonness406-AMP</Name>
  <Description>Application Management Panel from CubeCoders.com.&#13;
</Description>
  <Registry>https://hub.docker.com/r/othrayte/amp/~/dockerfile/</Registry>
  <Repository>othrayte/amp:develop</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name>LICENCE</Name>
      <Value>your licence key here</Value>
    </Variable>
    <Variable>
      <Name>MODULE</Name>
      <Value>ADS</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8080</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>path to config</HostDir>
      <ContainerDir>/ampdata</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version></Version>
  <WebUI>http://[IP]:[PORT:8080]</WebUI>
  <Banner></Banner>
  <Icon></Icon>
  <ExtraParams></ExtraParams>
</Container>
